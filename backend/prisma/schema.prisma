generator client {
provider = "prisma-client-js"
}


datasource db {
provider = "mysql"
url = env("DATABASE_URL")
}


model Entry {
id Int @id @default(autoincrement())
title String
type String
director String?
budget String?
location String?
duration String?
year String?
notes String?
posterUrl String?
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt
}